{
  "name": "progress-arc-component",
  "version": "2.5.0",
  "description": "Simple progress arc for React, customizable with styled-components",
  "main": "lib/ProgressArc.js",
  "repository": "https://github.com/szastupov/progress-arc-component.git",
  "author": "Stepan Zastupov <stepan.zastupov@gmail.com>",
  "keywords": [
    "react",
    "progress",
    "bar",
    "progress",
    "arc",
    "styled-components"
  ],
  "license": "MIT",
  "dependencies": {
    "babel-preset-env": "^1.6.1",
    "styled-components": "^3.1.6"
  },
  "peerDependencies": {
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "husky": "^0.14.3",
    "jest": "^22.1.4",
    "lint-staged": "^6.1.0",
    "prettier": "^1.5.3",
    "prop-types": "^15.5.10",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0"
  },
  "scripts": {
    "build": "babel src -d lib",
    "dev": "babel -w src -d lib",
    "prepublish": "npm run build",
    "test": "jest",
    "test:update": "jest -u",
    "precommit": "lint-staged"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --no-semi --write",
      "git add"
    ]
  }
}
